<h3>Student  Form</h3>
<%= form_for(@student) do |f| %>
  <div class="field">
    <%= f.label :first_name, 'First Name'%><br>
    <%= f.text_field :first_name %><br>
    <%= f.label :last_name, 'Last Name'%><br>
    <%= f.text_field :last_name %>
  </div>
  
  <div class= "actions">
    <%= f.submit %>
  </div>
<% end %> 